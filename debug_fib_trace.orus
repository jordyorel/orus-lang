fn fib(n) -> i32:
    print("fib(", n, ") called")
    if n <= 1:
        print("  returning", n)
        return n
    else:
        print("  computing fib(", n-1, ") + fib(", n-2, ")")
        left = fib(n - 1)
        print("  fib(", n-1, ") =", left)
        right = fib(n - 2)
        print("  fib(", n-2, ") =", right)
        result = left + right
        print("  fib(", n, ") =", result)
        return result

result = fib(4)
print("Final: fib(4) =", result, "(should be 3)")