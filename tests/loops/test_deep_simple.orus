// Test deep nesting without string operations
print("=== Deep Nesting Simple ===")
for a in 1..3:
    print(a)
    for b in 1..3:
        print(b)
        for c in 1..4:
            print(c)
            if c == 2:
                continue
            for d in 1..3:
                print(d)
                if d == 1:
                    continue
                if d == 2:
                    break
            if c == 3:
                break
        if b == 2:
            break
print("Complete")