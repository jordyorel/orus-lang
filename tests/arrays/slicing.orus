// Array slicing smoke test validating `[start..end]` syntax.
// Ensures slices treat the end index as inclusive and preserve element order.

print("-- array slicing --")
values = [5, 6, 7, 8, 9]

head = values[0..1]
print("head:", head)

mid = values[1..3]
print("mid:", mid)

full = values[0..len(values)]
print("full:", full)

upto_len = values[..len(values)]
print("upto len:", upto_len)

prefix = values[..2]
print("prefix:", prefix)

suffix = values[2..]
print("suffix:", suffix)

clone = values[..]
print("clone:", clone)

single = values[3..3]
print("single:", single)

empty = values[len(values)..len(values)]
print("empty:", empty)
