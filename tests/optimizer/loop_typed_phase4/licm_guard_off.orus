// Baseline copy of licm_guard.orus used for LICM-disabled telemetry comparison
mut threshold: i32 = 6
mut base_guard: bool = threshold < 12
mut result: i32 = 0
mut index: i32 = 0

while index < threshold:
    mut typed_guard: bool = base_guard
    mut fused_guard: bool = typed_guard and base_guard
    if fused_guard:
        result = result + index
    index = index + 1

print("phase4-off", result)
