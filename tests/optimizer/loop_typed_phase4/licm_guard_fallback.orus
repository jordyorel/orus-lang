mut limit: i32 = 6
mut base_guard: bool = limit < 10
mut sum: i32 = 0
mut index: i32 = 0

while index < limit:
    mut typed_guard: bool = base_guard
    if not typed_guard:
        break
    if index == 4:
        print("phase4-fallback-trigger", index)
        break
    sum = sum + index
    index = index + 1

print("phase4-fallback", sum)
