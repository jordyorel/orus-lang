mut limit: i32 = 4
mut base_guard: bool = limit < 10
mut total: i32 = 0
mut outer: i32 = 0

while outer < limit:
    mut outer_guard: bool = base_guard
    mut fused_outer: bool = outer_guard and base_guard
    mut inner: i32 = 0
    while inner < limit:
        mut inner_guard: bool = fused_outer
        if inner_guard:
            total = total + outer + inner
        inner = inner + 1
    outer = outer + 1

print("phase4-nested", total)
