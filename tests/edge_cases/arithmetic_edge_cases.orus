// Edge cases for arithmetic operations

// Zero operations
print(0 + 0)
print(0 - 0)
print(0 * 5)
print(5 * 0)

// Subtraction to simulate negative numbers
print(5 - 3)
print(15 - 10)

// Large numbers
print(999999 + 1)
print(1000000 - 1)

// Complex nested expressions
print(((1 + 2) * 3) - 4)
print(1 + (2 * (3 + 4)))
print((10 / 2) + (3 * 4))

// Division edge cases
print(1 / 1)
print(10 / 2)
print(7 / 2)  // Integer division?

// Chain operations
print(1 + 2 + 3 + 4)
print(10 - 3 - 2 - 1)
print(2 * 3 * 4)