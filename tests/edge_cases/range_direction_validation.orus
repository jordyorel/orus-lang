// Test range direction validation

// Test 1: Valid forward range with positive step
for i in 0..10..1:
    print(i)

// Test 2: Valid step of 2
for i in 0..10..2:
    print(i)

// Test 3: Edge case - same start and end
for i in 5..5..1:
    print(i)  // Should execute once or not at all

// Test 4: Edge case - very large step
for i in 0..10..20:
    print(i)  // Should print 0

// Test 5: Basic range without step
for i in 1..5:
    print(i)