// Edge cases for variable operations

// Single character variable names
a = 1
b = 2
c = a + b
print(c)

// Longer variable names
myVariable = 42
anotherLongVariableName = 100
print(myVariable)
print(anotherLongVariableName)

// Variable reassignment
x = 5
print(x)
x = 10
print(x)
x = x + 1
print(x)

// Using variables in complex expressions
first = 10
second = 20
result = (first + second) * 2
print(result)

// Variable used multiple times
value = 7
print(value + value)
print(value * value)
print(value - value)

// Variables with numbers in names (if allowed)
var1 = 100
var2 = 200
sum12 = var1 + var2
print(sum12)