// Deep nesting and complex expression edge cases

// Deep parentheses nesting
print(((((1 + 2) + 3) + 4) + 5))
print(((10 - 5) - 2) - 1)

// Mixed operations with deep nesting
print(((2 + 3) * (4 + 5)) / 7)
print((10 + (5 * 2)) - (8 / 2))

// Variables in deeply nested expressions
a = 2
b = 3
c = 4
print(((a + b) * c) + (a * (b + c)))

// Complex assignment with nested expressions
result = ((10 + 5) * 2) + ((20 - 10) / 2)
print(result)

// Chain of variable assignments
x = 1
y = x + 2
z = y * 3
w = z - 4
print(w)

// Expression reusing same variables
num = 5
print((num + num) * (num - 1))
print(num + (num * (num + 1)))