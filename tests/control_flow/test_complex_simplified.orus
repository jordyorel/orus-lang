// Simplified version of complex nested
print("Complex simplified test:")

for i in 0..=3:
    print("Level 1: i =", i)
    
    mut j = 0
    while j < 2:
        j = j + 1
        print("  Level 2: j =", j)
        
        for k in 0..2:
            print("    Level 3: k =", k)
        print("    Level 3 done")
    print("  Level 2 done")
print("All done")