// Function edge cases and boundary testing
fn no_params() -> i32:
    return 42

fn multiple_params(a: i32, b: i32, c: i32, d: i32) -> i32:
    return a + b + c + d

// Test functions with no parameters
result1 = no_params()
print("No params result:", result1)

// Test functions with multiple parameters
result2 = multiple_params(1, 2, 3, 4)
print("Multiple params result:", result2)

// Test nested function calls
fn square(x: i32) -> i32:
    return x * x

fn sum_of_squares(a: i32, b: i32) -> i32:
    return square(a) + square(b)

result3 = sum_of_squares(3, 4)
print("Sum of squares result:", result3)