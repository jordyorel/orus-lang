// Test first-class function features
fn add(a, b):
    return a + b

fn multiply(x, y):
    return x * y

// Test 1: Function assignment
addFunc = add
result1 = addFunc(5, 3)
print("Function assignment:", result1)

// Test 2: Function as variable
operation = multiply
result2 = operation(4, 6)
print("Function as variable:", result2)

// Test 3: Different function variables  
operation2 = add
result3 = operation2(10, 20)
print("Different function variable:", result3)