// Nested closures test - multiple levels of capture
fn make_adder(x: i32) -> fn(i32) -> i32:
    fn inner(y: i32) -> i32:
        x + y
    inner

add5 = make_adder(5)
add10 = make_adder(10)

print("add5(3): ", add5(3))
print("add10(7): ", add10(7))

// Test multiple captures
fn make_calculator(a: i32, b: i32) -> fn(i32) -> i32:
    fn calculate(x: i32) -> i32:
        (a * x) + b
    calculate

calc = make_calculator(2, 3)
print("calc(5): ", calc(5))  // Should be 13