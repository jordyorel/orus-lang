// Basic f64 boundary tests
zero: f64 = 0.0
one: f64 = 1.0
neg_one: f64 = 0.0 - 1.0

print(zero)
print(one)
print(neg_one)

// Basic arithmetic - types inferred from operands
result1 = zero + one
result2 = one * neg_one
print(result1)
print(result2)

// Test with very small and large numbers
tiny: f64 = 1e-10
huge: f64 = 1e10
print(tiny)
print(huge)

product = tiny * huge
print(product)