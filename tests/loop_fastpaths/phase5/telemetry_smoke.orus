mut total = 0
array: [i32, 3] = [1, 3, 5]
for value in array:
    total = total + value

mut idx = 0
while idx < 4:
    if idx == 2:
        idx = idx + 1
        continue
    idx = idx + 1

assert_eq("phase5 telemetry_smoke total", total, 9)
assert_eq("phase5 telemetry_smoke idx", idx, 4)
print("phase5-ok")
