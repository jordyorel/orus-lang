iterable = range(10)

mut first_pass = 0
mut first_total = 0

for value in iterable:
    first_total = first_total + value
    first_pass = first_pass + 1
    if first_pass == 4:
        break

mut second_pass = 0
mut second_total = 0
for value in iterable:
    second_total = second_total + value
    second_pass = second_pass + 1

assert_eq("phase3 iterator_range_resume first_pass", first_pass, 4)
assert_eq("phase3 iterator_range_resume first_total", first_total, 6)
assert_eq("phase3 iterator_range_resume second_pass", second_pass, 6)
assert_eq("phase3 iterator_range_resume second_total", second_total, 39)
print("phase3-range-resume-ok")
