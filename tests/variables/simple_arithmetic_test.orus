// Test basic arithmetic operations that should work
a: i64 = 1000000
b: i64 = 500000
total: i64 = a + b
product: i64 = a * 2
division: i64 = a / 1000
remainder: i64 = a % 7

print("Addition:", total)
print("Multiplication:", product) 
print("Division:", division)
print("Remainder:", remainder)

// Test f64 operations
x: f64 = 1.5
y: f64 = 2.5
sum: f64 = x + y
product_f: f64 = x * y
division_f: f64 = x / y

print("F64 sum:", sum)
print("F64 product:", product_f)
print("F64 division:", division_f)

// Test mixed precision
large_f: f64 = 1e10
small_f: f64 = 1e-10
mixed: f64 = large_f * small_f

print("Mixed precision:", mixed)
