// Test compile-time scope analysis and variable lifetime tracking

// Global scope variables  
mut x = 10
mut y = 20

// Test basic scope
if x > 5:
    mut inner_val = x + y
    print("inner_val:", inner_val)

// Test loop scope with lifetime tracking
for i in 0..5:
    mut loop_var = i * 2
    print("loop_var:", loop_var)
    
    if loop_var > 4:
        mut nested = loop_var + 1
        print("nested:", nested)

// Test variable reuse across scopes
if y > 15:
    mut another_temp = y - 10
    print("another_temp:", another_temp)

// Test while loop scope
while x > 0:
    x = x - 1
    mut counter = x
    print("counter:", counter)
    if x < 5:
        break

print("Scope analysis test completed")