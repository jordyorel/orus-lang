// Test recursive fibonacci with multiple values
fn fibonacci(n) -> i32:
    if n <= 1:
        return n
    
    left = fibonacci(n-1)
    right = fibonacci(n-2) 
    result = left + right
    return result

// Test multiple values
for i in 0..8:
    result = fibonacci(i)
    print("fibonacci(", i, ") =", result)

