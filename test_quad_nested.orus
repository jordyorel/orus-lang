// Test quad nested loops  
print("Testing quad nested:")

for i in 0..2:
    print("Level 1: i =", i)
    
    for j in 0..2:
        print("  Level 2: j =", j)
        
        mut k = 0
        while k < 2:
            k = k + 1
            print("    Level 3: k =", k)
            
            for l in 0..2:
                print("      Level 4: l =", l)
                if l == 1:
                    print("      Continue at l =", l)
                    continue
                print("      Processing l =", l)
            print("    Level 3 continues")
        print("  Level 2 continues")
    print("Level 1 continues")
print("All done")