// Test negative literals parsing edge cases
mut a = -1
mut b = -42i32
mut c = -123i64
mut d = -456u32
mut e = -789u64  
mut f = -3.14f64

print("Negative literals:")
print(a)   // -1
print(b)   // -42
print(c)   // -123  
print(d)   // Should this be allowed? Negative unsigned?
print(e)   // Should this be allowed? Negative unsigned?
print(f)   // -3.14

// Test type-specific negation
mut x_i32 = 100i32
mut neg_i32 = -x_i32
print("i32 negation:")
print(neg_i32)

mut x_i64 = 200i64
mut neg_i64 = -x_i64
print("i64 negation:")
print(neg_i64)

mut x_f64 = 3.14f64
mut neg_f64 = -x_f64
print("f64 negation:")
print(neg_f64)

// Test edge cases
mut zero = 0
print("Zero negation:")
print(-zero)

// Test not operator with different contexts
mut bool_var = false
print("Boolean negation:")
print(not bool_var)
print(not true)
print(not false)
