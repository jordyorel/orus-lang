// Test mixed loop types: WHILE (outer) → FOR (middle) → WHILE (inner) → IF with break/continue
mut outer = 0
while outer < 2:
    print(outer)
    for mid in 1..3:
        print(mid)
        mut inner = 0
        while inner < 2:
            print(inner)
            if inner == 0:
                inner = inner + 1
                continue
            break
        if mid == 1:
            break
    outer = outer + 1
print(999)