fn add_one(x) -> i32:
    return x + 1

fn add_two_numbers(a, b) -> i32:
    return a + b

print("add_one(5):", add_one(5))
print("add_two_numbers(3, 4):", add_two_numbers(3, 4))

fn fibonacci_step(n):
    if n <= 1:
        return n
    // Instead of recursive calls, use fixed values
    a = add_one(0)  // Should be 1 (like fibonacci(1))
    b = add_one(-1) // Should be 0 (like fibonacci(0))  
    return add_two_numbers(a, b)  // Should be 1

print("fibonacci_step(2):", fibonacci_step(2))