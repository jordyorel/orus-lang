// Test 2: Logical operations - isolated
print("Testing logical operations...")

// Simple logical tests first
print("true or false =", true or false)
print("false and true =", false and true)
print("not (false and true) =", not (false and true))

// The failing test condition
condition1 = true or false
print("condition1 (true or false) =", condition1)

condition2 = false and true  
print("condition2 (false and true) =", condition2)

condition3 = not condition2
print("condition3 (not condition2) =", condition3)

final_result = condition1 and condition3
print("final_result (condition1 and condition3) =", final_result)

// The exact test that should pass
test_result = (true or false) and not (false and true)
print("Full test: (true or false) and not (false and true) =", test_result)

if test_result:
    print("Test 2 PASSED: Logical operations")
else:
    print("Test 2 FAILED: Logical operations")