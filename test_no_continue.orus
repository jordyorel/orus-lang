// Test nested without continue
print("Testing without continue:")

for i in 0..2:
    print("Level 1: i =", i)
    
    mut j = 0
    while j < 3:
        j = j + 1
        print("  Level 2: j =", j)
        
        for k in 0..3:
            print("    Level 3: k =", k)
                
            mut l = 0
            while l < 2:
                l = l + 1
                print("      Level 4: l =", l)
                print("        Level 4 processing")
            print("      Level 4 done")
        print("    Level 3 done")
    print("  Level 2 done")
print("All complete")