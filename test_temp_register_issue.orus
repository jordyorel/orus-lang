fn get_one():
    print("get_one called")
    return 1

fn get_two():
    print("get_two called")
    return 2

// Test intermediate storage
print("Testing step by step:")
val1 = get_one()
print("val1 =", val1)
val2 = get_two()  
print("val2 =", val2)
result = val1 + val2
print("val1 + val2 =", result)

print()
print("Testing direct expression:")
direct = get_one() + get_two()
print("get_one() + get_two() =", direct)