// Test conditional logic in for loop
print("Conditional logic test:")

for i in 0..8:
    print("Loop start: i =", i)
    if i < 2:
        print("  Low i =", i)
    elif i == 3 or i == 5:
        print("  Skip i =", i)
        continue
    elif i == 7:
        print("  Break i =", i)
        break
    else:
        print("  Process i =", i)
    print("Loop end: i =", i)

print("Done")