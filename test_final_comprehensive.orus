// Final comprehensive test of break/continue
print("=== Final Comprehensive Test ===")

print("\n1. Simple for loop with break:")
for i in 0..5:
    if i == 3:
        print("Breaking at", i)
        break
    print("i =", i)

print("\n2. Simple for loop with continue:")
for i in 0..5:
    if i == 2:
        print("Skipping", i)
        continue
    print("Processing", i)

print("\n3. Nested for loops with break:")
for i in 0..3:
    print("Outer", i)
    for j in 0..4:
        if j == 2:
            print("  Inner break at", j)
            break
        print("  Inner", j)

print("\n4. Nested with continue:")
for i in 0..3:
    print("Outer", i)
    for j in 0..4:
        if j == 1:
            print("  Inner continue at", j)
            continue
        print("  Inner", j)

print("\n5. For-while mixed:")
for i in 0..2:
    print("For", i)
    mut k = 0
    while k < 3:
        k = k + 1
        if k == 2:
            print("  While continue at", k)
            continue
        print("  While", k)

print("\n=== All Tests Complete ===")