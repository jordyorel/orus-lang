// Comprehensive for loop test
print("=== Basic exclusive range ===")
for i in 1..4:
    print(i)

print("=== Inclusive range ===")  
for j in 5..=7:
    print(j)

print("=== Range with step ===")
for k in 10..20..3:
    print(k)

print("=== Single iteration ===")
for x in 42..=42:
    print(x)

print("All tests completed!")