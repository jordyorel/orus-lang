// Test 4-level nested for loops
print("Testing 4-level nesting:")
for i in 0..2:
    print("Level 1: i =", i)
    for j in 0..2:
        print("  Level 2: j =", j)
        for k in 0..2:
            print("    Level 3: k =", k)
            for l in 0..2:
                print("      Level 4: l =", l)
print("Done")