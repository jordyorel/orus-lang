fn fibonacci(n):
    print("fibonacci called with n =", n)
    if n <= 1:
        print("base case: returning", n)
        return n
    print("recursive case: computing fibonacci(", n-1, ") + fibonacci(", n-2, ")")
    return fibonacci(n - 1) + fibonacci(n - 2)

print("=== Testing fibonacci(3) ===")
result = fibonacci(3)
print("Final result:", result)