// Test triple nested loops
print("Testing triple nested:")

for i in 0..2:
    print("Level 1: i =", i)
    
    mut j = 0
    while j < 2:
        j = j + 1
        print("  Level 2: j =", j)
        
        for k in 0..2:
            print("    Level 3: k =", k)
            if k == 1:
                print("    Continue at k =", k)
                continue
            print("    Processing k =", k)
        print("  Level 2 continues")
    print("Level 1 continues")
print("All done")