// Debug case 4 issue
print("Debugging case 4:")

for i in 0..8:
    print("Start of loop: i =", i)
    if i < 2:
        print("  Branch 1: Low i =", i)
    elif i == 3 or i == 5:
        print("  Branch 2: Skip i =", i)
        continue
    elif i == 7:
        print("  Branch 3: Break i =", i)
        break
    else:
        print("  Branch 4: Process i =", i)
    print("End of loop iteration for i =", i)

print("Loop finished")