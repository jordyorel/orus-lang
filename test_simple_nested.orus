// Test simple nested loops
print("Testing simple nested:")

for i in 0..2:
    print("Outer i =", i)
    mut j = 0
    while j < 3:
        j = j + 1
        print("  Inner j =", j)
        if j == 2:
            print("  Continue at j =", j)
            continue
        print("  Processing j =", j)
    print("Outer continues")
print("Done")