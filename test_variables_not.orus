// Test with variables to avoid constant folding
print("Testing with variables to avoid constant folding...")

p = true
q = false

// Test step by step
step1 = p or q
print("p or q =", step1, "Expected: true")

step2 = q and p  
print("q and p =", step2, "Expected: false")

step3 = not step2
print("not (q and p) =", step3, "Expected: true")

final = step1 and step3
print("(p or q) and not (q and p) =", final, "Expected: true")