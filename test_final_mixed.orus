// Final test of mixed break/continue
print("Mixed break/continue test:")

for i in 0..8:
    print("Loop i =", i)
    if i == 3 or i == 5:
        print("  Continue at i =", i)
        continue
    if i == 7:
        print("  Break at i =", i)  
        break
    print("  Processing i =", i)

print("Test complete")