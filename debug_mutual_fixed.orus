fn func_b(n: i32) -> i32:
    print("func_b called with", n)
    if n <= 1:
        return 2  
    else:
        result = func_a(n - 1)  
        print("func_b: func_a returned", result)
        return result + 2

fn func_a(n: i32) -> i32:
    print("func_a called with", n)
    if n <= 1:
        return 1
    else:
        result = func_b(n - 1)
        print("func_a: func_b returned", result)
        return result + 1

print("Testing mutual recursion:")
final = func_a(3)
print("Final:", final)