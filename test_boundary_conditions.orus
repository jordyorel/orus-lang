// Test boundary conditions

// Test 1: Large range with step
print("=== Test 1: Large Range ===")
mut count = 0
for i in 1..1000..100:
    count = count + 1
    if count > 3:
        break
    print(i)

// Test 2: Single iteration
print("=== Test 2: Single Iteration ===")
for i in 5..6:
    print("Single: ", i)

// Test 3: Inclusive vs exclusive ranges
print("=== Test 3: Inclusive vs Exclusive ===")
print("Exclusive 1..3:")
for i in 1..3:
    print(i)
print("Inclusive 1..=3:")
for i in 1..=3:
    print(i)

// Test 4: Break/continue in deeply nested loops
print("=== Test 4: Deep Nesting Control ===")
mut outer = 0
while outer < 2:
    for i in 1..3:
        for j in 1..3:
            if i == 1 and j == 2:
                continue
            if i == 2 and j == 2:
                break
            print("(" + (i as string) + "," + (j as string) + ")")
    outer = outer + 1

print("All boundary tests complete")