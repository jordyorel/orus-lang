fn get_value(n) -> i32:
    if n <= 0:
        return 5
    else:
        return get_value(n - 1)

fn test_recursive() -> i32:
    x = 42
    print("Before recursive call: x =", x)
    y = get_value(2)
    print("After recursive call: x =", x, ", y =", y)
    result = x + y
    print("x + y =", result)
    return result

result = test_recursive()
print("Final result:", result, "(should be 47)")