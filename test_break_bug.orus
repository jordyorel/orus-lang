// Test break execution issue
print("Testing break execution:")

for k in 0..3:
    print("k =", k)
    
    if k == 2:
        print("Breaking at k =", k)
        break
        
    mut l = 0
    while l < 2:
        l = l + 1
        print("  l =", l)
    print("  After while loop")
print("After for loop")