// Test 5-level nested for loops
print("Testing 5-level nesting:")
for i in 0..2:
    print("Level 1: i =", i)
    for j in 0..2:
        print("  Level 2: j =", j)
        for k in 0..2:
            print("    Level 3: k =", k)
            for l in 0..2:
                print("      Level 4: l =", l)
                for m in 0..2:
                    print("        Level 5: m =", m)
print("Done")